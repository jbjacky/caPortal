
.information_row{
    padding:0px 20px;
    margin-right: 0px;
    margin-left: 0px;
    padding-left: 0px;
    padding-right: 0px;
}
.writerow{
    padding-left: 20px;
    padding-right: 0px
}
.inputTextarea{
    width: 100%;
    resize: none;
    height:83px;
    margin-top: 5px;
    display: block;
    padding: 6px 12px;
    font-size: 15px;
    line-height: 1.5;
    color: #555555;
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    border-radius: 2px;
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
}
.oneintwotitle{
    text-align: right
}
.D_button{
    cursor: pointer;
    display: inline;
}
.P_button{
    display: none;
}
.D_img{
    display: inherit;
}
.P_img{
    display: none;
}
.D_center{
    text-align: center;
}
.P_show{
    display: none;
}
.D_right{
    text-align:right;
}
.br_phone{
    display: none;
}
.errorInput{
    border: 1px solid #e60012
}
.col-md-5ths{
    position: relative;
    min-height: 1px;
    text-align: center;
    height: 70px;
}
.col-md-5ths-title{
    background-color: #edf5ff;
    border: #e0e0e0 1px solid;
    font-weight: bold;
    height: 50%;
    line-height: 35px;
}
.col-md-5ths-content{
    border: #e0e0e0 1px solid;
    height: 50%;
    line-height: 35px;
}

input:disabled+span {
    color: #ccc;
  }

@media (min-width: 768px) {
    .col-md-5ths {
        width: 20%;float: left;
    }
}

@media screen and (min-width: 1120px) and (max-width: 1280px) {
    .col-md-5ths {
        width: 25%;float: left;
    }
}

@media screen and (min-width: 992px) and (max-width: 1120px) {
    .col-md-5ths {
        width: 33%;float: left;
    }
}

@media screen and (min-width: 768px) and (max-width: 992px) {
    .col-md-5ths {
        width: 100%;float: left;
    }
}


@media screen and (max-width:992px) {
    .stateblock{
        display: none;
    }
    .first-form-group{
        padding-bottom: 5px
    }
    .firstinone-form-group{
        padding-bottom: 10px;
     border-bottom: 1px dashed #dcdcdc;
     } 
     .firstintwo-form-group{
         padding-top:10px
     }
     .oneintwotitle{
        text-align: left;
        padding: 0px
    }
     .oneintwocontent{
         padding: 0px
     }
     .writerow{
        padding-left: 0px;
        padding-right: 0px
    }
    
    .D_button{
        display: none;
    }
    .P_button{
        display: inherit;
        float: right
    }
    .D_img{
        display: none;
    }
    .P_img{
        display: inherit;
    }
    .D_center{
        text-align: left;
    }
    
    .P_show{
        display: inherit;
    }
    .D_right{
        text-align:left;
    }
    .br_phone{
        display: inherit;
    }
    
    .P_inf{
        overflow-y: auto;
        height:200px
    }
}
    