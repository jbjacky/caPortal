


@media (min-width: 992px){
    .row.rowmargin.col2change .col-md-2  {
      width: 13%;
    }
    .two-seven-cols .col-md-2 {
      width: 12%;
      padding: 2px;
    }
    .col-md-12{
      padding: 0;
    }
  }
  
.P_left{
    text-align: right;
}
  
  .DivStyle{
    border:1px solid #e0e0e0;
    border-radius:5px 
  }
  
  .DivDateStyle{
    background-color:#edf5ff;
    border-radius:5px;
    text-align: center;
    padding: 20px 0px;
    margin-bottom: 5px
  }
  .oneDivJobStyle{
    text-align: center;
    padding: 21px 0px;
    border-radius:5px;
    margin-bottom: 5px;
    border-bottom: 1px solid #e6e6e6;
  }
  .twoDivJobStyle{
    text-align: center;
    padding: 21px 0px;
    border-radius:5px;
    margin-bottom: 5px
  }
  
  
  .selectDiv{
    box-shadow: 0 2px 9px 0 rgba(123, 123, 123, 0.5);
    border: 1px solid #4a90e2;
    cursor: pointer;
  }
  .selectDateDiv{
    color:  #4a90e2;
  }
  
  .selectDateDiv_check{
    color:  #fff;
    background-color: #0068b7;
  }
  
  .selectJobDiv{
    color: #fff;
    background-color: #64aadf
  }
  .P_right{
    padding: 0px;
    text-align: right;
  }
  .P_zero{
    padding: 0;
    padding-left: 15px
  }
  .P_LFZero{
    padding-left: 15px;
    padding-right: 15px
  }
  .onShowButton{
    color: #fff;
    background: 0;
    border: 0;
    background-color: #006dbc;
    padding: 4px 10px;
  }
  .offShowButton{
    color: #028fcf;
    background: 0;
    background-color: #fff;
    padding: 4px 10px;
    border: 1px solid #c3c3c3;
  }
  .a-block-buttom {
      width: 850px;
      margin: 0 auto;
      
  }
  @media screen and (max-width:992px) {
  
    .stateblock{
      display: none;
  }
    .oneDivJobStyle{
      border-bottom:0px
    }
    .P_right{
      text-align: left
    }
    .P_zero{
      padding: 0;
    }
    .P_LFZero{
      padding-left: 0px;
      padding-right: 0px
    }
    
    .P_left{
        text-align: left;
    }
  }
  
  
    