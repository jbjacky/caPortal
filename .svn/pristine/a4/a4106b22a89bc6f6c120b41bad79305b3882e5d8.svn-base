::ng-deep .slick-prev:before {
    display:none;
}
::ng-deep  .slick-next:before {
    display:none;
}

::ng-deep .slick-prev{
    left: -30px;
    width: 50px;
    height: 50px;
    background: url(../../../assets/images-template/icon-left.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep  .slick-prev:hover {
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-left.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep .slick-prev:focus{
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-left.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep .slick-next{
    right: -45px;
    width: 50px;
    height: 50px;
    background: url(../../../assets/images-template/icon-right.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep  .slick-next:hover {
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-right.png) ;
    background-repeat:no-repeat;
}

::ng-deep   .slick-next:focus{
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-right.png) ;
    background-repeat:no-repeat;
    
}

.Atooltips .tooltiptext {
    visibility: hidden;
    background-color: black;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;

    /* Position the tooltip */
    position: absolute;
    z-index: 1;
}

.Atooltips:hover .tooltiptext {
    visibility: visible;
}


