.P_base{
  color: #6f6f6f;
  border-right: 1px solid rgba(151, 151, 151, 0.32);
}
.P_date{
  padding: 0px;
  
  text-align: center;
  /* padding-left: 20px; */
}

.P_line{
  margin: 0px;
}
.errorInput{
  border: 1px solid #e60012
}
.P_div{
  border:1px solid #028fcf;
  margin: 0px;
  margin-bottom:10px;
  padding: 15px 10px;
  padding-top: 20px;
}

.P_padding{
  padding: 0px;
  padding-right: 20px;
}
.P_left{
  text-align: right
}
.P_All{
  text-align: center;
  font-weight: bold;
}
@media screen and (max-width:992px) {
.P_base{
  color: #6f6f6f;
  border-right:0px;
  border-bottom: 1px solid rgba(151, 151, 151, 0.32);
  text-align: unset;
}
.P_date{
  padding: 0px;
  padding-top: 10px;
}
.P_line{
  border-bottom: 2px solid #999999;
  margin: 0px;
  padding: 10px 0px;
  margin-bottom: 10px;
}

.P_div{
  padding: 10px 15px;
}
.P_padding{
  padding: 0px;
  padding-right: 0px;
}
.P_left{
  text-align: left
}
.P_All{
  text-align: left
}
}


