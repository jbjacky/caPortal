.onenewsblock {
    border: 1px solid #e0e0e0;
    padding: 10px 20px;
}
.onenewsblock :hover{
    color: #4a90e2;
    cursor: pointer
}
.checkform{
    background-image: linear-gradient(to left, #4e80bf, #02599b);
    box-shadow: 0 2px 3px 0 rgba(0, 0, 0, 0.1);
    padding: 5px 5px;
    cursor: pointer
}
.checkformdesktoptext{
    color: #fff;font-size: 17px;
}
.managerblock{
    background-image: linear-gradient(to left, #508598, #477bbd);
    box-shadow: 0 2px 3px 0 rgba(0, 0, 0, 0.1);
    padding: 5px 5px;
}
.mangerblockleft{
    padding-left: 15px;
    padding-right: 5px;
}
.managerblockright{
    padding-left: 5px;
    padding-right: 15px;
}
.mangerblocklefthourtitle{
    font-size: 14px;
    display: inline;
}
.myvacationblock{
    background-color: #fff;
    padding: 30px 40px;
}
