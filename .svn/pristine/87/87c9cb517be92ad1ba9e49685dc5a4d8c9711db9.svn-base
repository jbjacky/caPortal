}
.slideblock{
    padding-left: 15px;
    padding-right: 15px;
    margin-bottom: 20px;
}
.weekdate{
    border:1px solid #c9c9c9;
    border-right: 0px;
    width: 140px;
    text-align: center;
    padding: 3px 25px;
    background-color:#f9f9f9;
    border-bottom:0px;
    font-weight: 600
}
.weekdatecontent{
    border:1px solid #c9c9c9;
    border-right: 0px;
    width: 140px;
    text-align: center;
    padding: 30px 25px
}
::ng-deep .slick-prev:before {
    display:none;
}
::ng-deep  .slick-next:before {
    display:none;
}

::ng-deep .slick-prev{
    left: -30px;
    width: 50px;
    height: 50px;
    background: url(../../../assets/images-template/icon-left.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep  .slick-prev:hover {
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-left.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep .slick-prev:focus{
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-left.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep .slick-next{
    right: -45px;
    width: 50px;
    height: 50px;
    background: url(../../../assets/images-template/icon-right.png) ;
    background-repeat:no-repeat;
    
}
::ng-deep  .slick-next:hover {
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-right.png) ;
    background-repeat:no-repeat;
}

::ng-deep   .slick-next:focus{
    color: #f00;
    outline:none;
    background: url(../../../assets/images-template/icon-right.png) ;
    background-repeat:no-repeat;
    
}

.Atooltips .tooltiptext {
    visibility: hidden;
    background-color: black;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;

    /* Position the tooltip */
    position: absolute;
    z-index: 1;
}

.Atooltips:hover .tooltiptext {
    visibility: visible;
}
@media screen and (max-width:968px) {
    .checkformdesktoptext{
        display: none;
    }
    .mangerblockleft{    
        padding-left: 15px;
        padding-right: 15px;
        margin-bottom: 10px
    }
    .managerblockright{    
        padding-left: 15px;
        padding-right: 15px;
    }
    
    .mangerblocklefthourtitle{
        display: none;
    }
    
    .dottedline{
        border-bottom: 2px dotted #c3c3c3;
        margin: 0px 15px;
        margin-bottom: 25px;
    }
    /* .desktopmyvacationblock{display: none} */
    .phonemyvacationblock{display: block}
    .myvacationblock{
        padding: 30px 25px;
    }
    
    .P_inf{
        overflow-y: auto;
        height:200px
    }
    .leftmyvacation{
        border:1px solid #e0e0e0;
