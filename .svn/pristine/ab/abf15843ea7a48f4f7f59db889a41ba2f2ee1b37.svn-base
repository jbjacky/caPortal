<div *ngIf="dateworks.length>0">
  <div>
    <div style="text-align: center">
      <button class="bt" (click)="lessMonth(dateworks)">
        <img style="height: 20px;width: 20px;" src="./assets/images-template/icon-left.png" />
      </button>
      <span style="color: #028fcf;font-size: 18px">{{Year}}年{{Month}}月</span>
      <button class="bt" (click)="addMonth(dateworks)">
        <img style="height: 20px;width: 20px;" src="./assets/images-template/icon-right.png" />
      </button>
    </div>
    <!-- {{SearchMan.jobID}}{{SearchMan.name}}的班表 -->
